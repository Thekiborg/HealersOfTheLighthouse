<?xml version="1.0" encoding="UTF-8"?>
<Patch>

    <!-- Left Antenna -->
    <Operation Class="PatchOperationAdd">
        <xpath>Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Moyo"]/alienRace/generalSettings/
            alienPartGenerator/bodyAddons/li[name="moyo left antennae"]</xpath>
        <value>
            <extendedGraphics>
                <Hediff For="HOTL_RapidMetabolicBreakDown">Moyo/AntennaOverride/MoyoHornTall/MoyoHorn_Left</Hediff>
                <Gene For="HOTL_RapidMetabolicBreakDown">Moyo/AntennaOverride/MoyoHornShort/MoyoHorn_Left</Gene>
            </extendedGraphics>
        </value>
    </Operation>
    <Operation Class="PatchOperationAdd">
        <xpath>Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Moyo"]/alienRace/generalSettings/
            alienPartGenerator/bodyAddons/li[name="glow(moyo left antennae)"]</xpath>
        <value>
            <extendedGraphics>
                <Gene For="HOTL_RapidMetabolicBreakDown">Moyo_Dummy/Dummy</Gene>
            </extendedGraphics>
        </value>
    </Operation>

    <!-- Right Antenna -->
