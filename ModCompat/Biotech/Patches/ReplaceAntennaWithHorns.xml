            <extendedGraphics>
                <Hediff For="HOTL_RapidMetabolicBreakDown">Moyo/AntennaOverride/MoyoHornTall/MoyoHorn_Right</Hediff>
                <Gene For="HOTL_RapidMetabolicBreakDown">Moyo/AntennaOverride/MoyoHornShort/MoyoHorn_Right</Gene>
            </extendedGraphics>
        </value>
    </Operation>
    <Operation Class="PatchOperationAdd">
        <xpath>Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Moyo"]/alienRace/generalSettings/
            alienPartGenerator/bodyAddons/li[name="glow(moyo right antennae)"]</xpath>
        <value>
            <extendedGraphics>
                <Gene For="HOTL_RapidMetabolicBreakDown">Moyo_Dummy/Dummy</Gene>
            </extendedGraphics>
        </value>
    </Operation>

</Patch>