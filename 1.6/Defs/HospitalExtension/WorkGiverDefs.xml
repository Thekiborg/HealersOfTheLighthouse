<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <WorkGiverDef>
        <defName>HOTL_SurgeonTendEmergency</defName>
        <label>tend to patients with urgent needs</label>
        <giverClass>WorkGiver_TendOtherUrgent</giverClass>
        <workType>HOTL_Surgeon</workType>
        <emergency>true</emergency>
        <verb>tend to</verb>
        <gerund>tending to</gerund>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
        <priorityInType>20</priorityInType>
        <prioritizeSustains>true</prioritizeSustains>
    </WorkGiverDef>

    <WorkGiverDef>
        <defName>HOTL_MoveInAndOutOperatingBeds</defName>
        <label>take people to operating beds</label>
        <giverClass>HealersOfTheLighthouse.Workgiver_PopulateOperatingBeds</giverClass>
        <workType>Doctor</workType>
        <priorityInType>30</priorityInType>
        <billGiversAllAnimals>true</billGiversAllAnimals>
        <billGiversAllAnimalsCorpses>true</billGiversAllAnimalsCorpses>
        <verb>operate on</verb>
        <gerund>operating on</gerund>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
        <prioritizeSustains>true</prioritizeSustains>
    </WorkGiverDef>

</Defs>