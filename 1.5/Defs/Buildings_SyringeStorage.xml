<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <ThingDef Name="Thek_BoosterVerticalDispenser" ParentName="FurnitureBase" Abstract="True">
        <thingClass>Building_Storage</thingClass>
        <passability>PassThroughOnly</passability>
        <fillPercent>0.4</fillPercent>
        <pathCost>50</pathCost>
        <defaultPlacingRot>South</defaultPlacingRot>
        <altitudeLayer>PawnUnused</altitudeLayer>
        <drawGUIOverlay>True</drawGUIOverlay>
        <graphicData>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>(1.5,1.5)</drawSize>
            <drawOffsetNorth>(0,0,0.1)</drawOffsetNorth>
            <drawOffsetSouth>(0,0,-0.1)</drawOffsetSouth>
            <drawOffsetWest>(0.1,0,0)</drawOffsetWest>
            <drawOffsetEast>(-0.1,0,0)</drawOffsetEast>
            <drawRotated>false</drawRotated>
        </graphicData>
        <size>(1,1)</size>
        <costList>
            <Moyo2_Abyssteel>90</Moyo2_Abyssteel>
        </costList>
        <statBases>
          <Flammability>1.0</Flammability>
          <Beauty>10</Beauty>
        <MaxHitPoints>100</MaxHitPoints> <!-- Importante -->
        <Mass>8</Mass>
        <WorkToBuild>500</WorkToBuild> <!-- Importante -->
        </statBases>
        <building>
          <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
          <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
          <maxItemsInCell>4</maxItemsInCell>
          <blueprintClass>Blueprint_Storage</blueprintClass>
          <paintable>false</paintable>
          <storageGroupTag>Shelf</storageGroupTag>
          <fixedStorageSettings>
            <filter>
              <disallowNotEverStorable>true</disallowNotEverStorable>
            </filter>
          </fixedStorageSettings>
          <defaultStorageSettings>
            <priority>Important</priority>
          </defaultStorageSettings>
        </building>
        <inspectorTabs>
          <li>ITab_Storage</li>
        </inspectorTabs>
        <staticSunShadowHeight>0.5</staticSunShadowHeight>
        <surfaceType>Item</surfaceType>
        <canOverlapZones>false</canOverlapZones>
        <researchPrerequisites>
          <li>ComplexFurniture</li>
        </researchPrerequisites>
        <designationHotKey>Misc12</designationHotKey>
        <designatorDropdown>Thek_BoosterVerticalDispenser_DesignatorDropdown</designatorDropdown>
      </ThingDef>

